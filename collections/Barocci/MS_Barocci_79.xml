               <msContents>
                  <msItem n="1" xml:id="MS_Barocci_79-item1">
                     <author key="person_7524651">Aristotle</author>
                     <title key="work_669">Physica</title>
                     <note>Fol. 123v blank.</note>
                     <textLang mainLang="grc">Greek</textLang>
