               <msContents resp="#VD">
                  <msItem class="#liturgica" n="1" xml:id="MS_Rawl_D_1225-item1">
                     <title key="work_13140" type="desc">Martyrology</title>
                     <!-- check whether further indexing of content is required -->
                     <msItem>
                        <locus>(fol. 2, see also 10v)</locus>
                        <note>Prose, added 14th century (?).</note>
                        <incipit>Mittit ad virginem</incipit>
                     </msItem>
                     <msItem>
                        <locus from="2v">(fol. 2v)</locus>
                        <note>Note of rents payable to the light in the choir of St Chad's.</note>
                     </msItem>
                     <msItem>
                        <locus from="3">(fol. 3)</locus>
                        <note>Obituary, added 14th century.</note>
                     </msItem>
                     <msItem>
                        <locus from="9">(fol. 9)</locus>
                        <note>Sequence, added 14th century.</note>
                        <incipit>Psallat celum plaudit tellus resurgenti domino</incipit>
                        <note resp="#MLH">Pr. <title>Analecta hymnica</title> XL.84</note>
                     </msItem>
                     <msItem resp="#Q">
                        <locus from="9v">(fol. 9v)</locus>
                        <note>Lists of the (arch)bishops of Canterbury and Chester, late 12th century, continued respectively to 1278 to 1419.</note>
                     </msItem>
                     <msItem>
                        <locus from="10">(fol. 10)</locus>
                        <note>List of the relics in the shrine of St Chad, in Latin and English, added 14th century.</note>
                     </msItem>
                     <msItem>
                        <locus from="12">(fol. 12)</locus>
                        <author key="person_264282437">Usuardus</author>
                        <title key="work_4890">Martyrologium</title>
                        <note>Martyrology with later additions (extracts given in the Quarto Catalogue) including obits and historical notes continuing to the sixteenth century.</note>
                     </msItem>
                     <msItem>
                        <locus from="128">(fol. 128)</locus>
                        <note>Sequence, added 13th century.</note>
                        <incipit>Flos excellens flos beatus</incipit>
                        <note resp="#MLH">Pr. <title>Analecta hymnica</title> XL.79</note>
                     </msItem>
                     <msItem>
                        <locus from="129">(fol. 129)</locus>
                        <note>Sequence, added 13th century.</note>
                        <rubric>De sancta margareta</rubric>
                        <incipit>Flos convallis est egressus</incipit>
                        <note resp="#MLH">Pr. <title>Analecta hymnica</title> XL.246</note>
                     </msItem>
                     <msItem>
                        <locus from="130" to="133">(fol. 130–133)</locus>
                        <note>Four leaves with the outlines of an obituary, written by the scribe of the martyrology, but used for additional pieces of chant.</note>
                        <msItem resp="#Q">
                           <incipit>Alleluya. Matris Christi sunt amande Laudes nobis et cantande</incipit>
                        </msItem>
                        <msItem>
                           <locus from="130v">(fol. 130v)</locus>
                           <incipit>Gloriosa dei cella</incipit>
                        </msItem>
                        <msItem resp="#Q">
                           <incipit>Ave Dei genitrix Maria virgo, singularis nostre forma salutis</incipit>
                        </msItem>
                        <msItem resp="#Q">
                           <incipit>Alleluya. Spes Maria peccatoris Actu menta crebris horis</incipit>
                        </msItem>
                        <msItem>
                           <locus from="131">(fol. 131)</locus>
                           <incipit>Salve signum sancte crucis</incipit>
                           <incipit>Salve virgo sacra parens</incipit>
                           <note>Under the same melody.</note>
                        </msItem>
                        <msItem resp="#Q">
                           <locus from="132">(fol. 132)</locus>
                           <incipit>O vir magne pietatis Huc indulge tuis natis</incipit>
                           <note>To St Chad.</note>
                        </msItem>
                        <msItem>
                           <locus from="132v">(fol. 132v)</locus>
                           <incipit>Virgo cruces tres tortores</incipit>
                        </msItem>
                        <msItem>
                           <locus from="133">(fol. 133)</locus>
                           <incipit>Ave Maria pretiosa gemma</incipit>
                        </msItem>
                        <msItem resp="#Q">
                           <locus from="133v">(fol. 133v)</locus>
                           <incipit>O Edmunde singularis Signa ferens gratie</incipit>
                           <note>To St Edmund.</note>
                        </msItem>
                     </msItem>
                     <textLang mainLang="la" otherLangs="enm">Latin with a little Middle English (fol. 10)</textLang>
                  </msItem>
               </msContents>
