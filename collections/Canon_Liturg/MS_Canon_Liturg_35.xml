               <msContents>
                  
                  <msItem class="#liturgica" n="1" xml:id="MS_Canon_Liturg_35-item1">
                     <locus>(fols 1, 78)</locus>
                     <title key="work_10641" type="desc">Breviary, Roman</title>
                     <rubric>Commune Sanctorum, secundum consuetudinem Curie Romane</rubric>
                     <note>Part of a Breviary, consisting of:</note> 
                        
                        <msItem>
                           <locus>(fol. 1)</locus>
                           <title key="work_10687" type="desc">Calendar</title>
                           <note>On 16th April is 'Caustri martyris' and on 7th May 'Domicille virginis'</note>
                        </msItem>
                     
                     <msItem>
                        <locus>(fol. 13)</locus>
                        <title type="desc">Common of the Saints</title>
                     </msItem>
                        
                     <textLang mainLang="la">Latin</textLang>
                  </msItem>
                  
                  <msItem class="#liturgica" n="2" xml:id="MS_Canon_Liturg_35-item2">
                     <title key="work_13112" type="desc">Manual</title>
                     <msItem>
                        <note>Extracts, consisting of:</note>
                        
                        <msItem>
                           <title type="desc">'Ordo ante celebrandum'</title>
                        </msItem>
                        
                        <msItem>
                           <locus>(fol. 71)</locus>
                           <title type="desc">Prayers during the elevation</title>
                        </msItem>
                        
                        <msItem>
                           <locus>(fol. 75)</locus>
                           <title type="desc">Burial service of children</title>
                           <rubric>Ordo ad sepeliendum pueros</rubric>
                        </msItem>
                        
                        <msItem>
                           <locus>(fol. 75v)</locus>
                           <title type="desc">Order for introducing a woman into the church</title>
                           <rubric>Ordo ad introducendum mulierem in ecclesia</rubric>
                        </msItem>
                        
                        <msItem>
                           <locus>(fol. 76v)</locus>
                           <title type="desc">Order for the tonsure of children</title>
                           <rubric>Ad tondendum capillos pueris</rubric>
                        </msItem>
                        
                        <msItem>
                           <locus>(fol. 77v)</locus>
                           <title type="desc">Communion of the sick</title>
                           <note>Only a few lines left</note>
                        </msItem>
                        
                        <msItem>
                           <locus>(fol. 78)</locus>
                           <title type="desc">Common of the saints for Eastertide</title>
                        </msItem>
                        
                        <msItem>
                           <locus>(fol. 83v)</locus>
                           <title type="desc">Psalms</title>
                           <note>Incomplete</note>
                        </msItem>
                        
                        <msItem>
                           <locus>(fol. 91)</locus>
                           <title type="desc">Blessing of holy water</title>
                        </msItem>
                        
                     </msItem>

