               <msContents>
                  <!-- 
                   <date>After 1462</date>
  
                  -->
                  <msItem class="#liturgica" n="1" xml:id="MS_Canon_Liturg_220-item1">
                     <title key="work_10565" type="desc">Book of Hours, Use of Rome</title>
                     
                     <msItem>
                        <locus>(fol. 5)</locus>
                        <title type="desc">Penitential Psalms with Litany (fol. 21)</title>
                     </msItem>
                     
                     <msItem>
                        <locus>(fol. 35)</locus>
                        <rubric>Offitium in agenda Mortuorum</rubric>
                     </msItem>
                     
                     <msItem>
                        <locus>(fol. 78)</locus>
                        <title type="desc">Offitium sancte Crucis</title>
                     </msItem>
                     
                     <msItem>
                        <locus>(fol. 84)</locus>
                        <title type="desc">Offitium Sancti Spiritus</title>
                     </msItem>
                     
                     <msItem>
                        <locus>(fol. 92v)</locus>
                        <title type="desc">Prayer O domine J.C. adoro te, introduced by a long rubric mentioning the popes who gave indulgences</title>
                     </msItem>
                     <msItem>
                        <locus>(fol. 98)</locus>
                        <title type="desc">Mass of the Virgin and St Raphael (fol. 107v) with devotions (fol. 116v)</title>
                     </msItem>
                     <msItem>
                        <locus>(fol. 120v)</locus>
                        <title type="desc">Mass pro pestilentia (Recordare domine)</title>
                     </msItem>
                     <msItem>
                        <locus>(fol. 127)</locus>
                        <title type="desc">Illumina oculos, a commemoration of St Julian the hospitaller (131v) and other prayers.</title>
                     </msItem>
                     
                     <msItem>
                        <note>From rubrics on fol. 36v, etc. it appears that the office of the Virgin preceded the present manuscript.</note>
                        <note>On fols. 130v and 139 are prayers mentioning famulus tuus X.</note>
                     </msItem>
                     
                     <textLang mainLang="la">Latin</textLang>
                  </msItem>
                  
               </msContents>
