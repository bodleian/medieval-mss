                  <objectDesc form="codex">
                     <supportDesc material="perg">
                        <support>parchment</support>
                        <extent> <seg type="leaf"><measure type="laterEndleaf" quantity="2">ii (17th cent., fol. i paper)</measure> + <measure type="leaf" quantity="117">117</measure> + <measure type="laterEndleaf" quantity="2">ii (17th cent., fol. 119 paper)</measure></seg> <dimensions type="leaf" unit="mm">
