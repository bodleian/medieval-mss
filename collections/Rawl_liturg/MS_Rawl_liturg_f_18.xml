               <msContents>
                  <msItem class="#liturgica" xml:id="MS_Rawl_liturg_f_18-item1">
                     <title key="work_10551" type="desc">Book of Hours, Use of Paris</title>
                     <msItem n="1">
                        <note>Kalendar in French (fol. 1), a saint for each day</note>
                     </msItem>
                     <msItem n="2">
                        <note>Pericopes from the Gospels (fol. 13)</note>
                     </msItem>
                     <msItem n="3">
                        <note>Obsecro te (fol. 18), and O intemerata (fol. 21v)</note>
                     </msItem>
                     <msItem n="4">
                        <note>Office of the Virgin (fol. 24)</note>
                     </msItem>
                     <msItem n="5">
                        <note>Penitential Psalms (fol. 81) with litany (fol. 92v)</note>
                     </msItem>
                     <msItem n="6">
                        <note>Offices of the cross (fol. 97) and the Holy Ghost (fol. 100)</note>
                     </msItem>
                     <msItem n="7">
                        <note>Office of the dead (fol. 103)</note>
                     </msItem>
                     <msItem n="8">
                        <note>Fifteen Joys (fol. 130v: 'Les xv <sic>voies</sic> Nostre Dame'), in French</note>
                     </msItem>
                     <msItem n="9">
                        <note>Seven Requests (fol. 136: V Playes).</note>
                     </msItem>
                     <textLang mainLang="la" otherLangs="frm">Latin, Middle French</textLang>
                  </msItem>
               </msContents>
