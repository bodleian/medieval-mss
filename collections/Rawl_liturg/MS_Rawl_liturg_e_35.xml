               <msContents>
                  <msItem class="#liturgica" xml:id="MS_Rawl_liturg_e_35-item1">
                     <title key="work_10551" type="desc">Book of Hours, Use of Paris</title>
                     <msItem>
                        <note>Kalendar in French (fol. 1), a saint for each date</note>
                     </msItem>
                     <msItem>
                        <note>Pericopes from the gospels (fol. 13)</note>
                     </msItem>
                     <msItem>
                        <note>Obsecro te (fol. 17)</note>
                     </msItem>
                     <msItem>
                        <note>Fifteen Joys (fol. 21), Quicumque vult (fol. 25v), and Seven Requests
                           (fol. 26), all in French</note>
                     </msItem>
                     <msItem>
                        <note>Commemorations (fol. 29v)</note>
                     </msItem>
                     <msItem>
                        <note>Office of the Virgin (fol. 32)</note>
                     </msItem>
                     <msItem>
                        <note>Offices of the Cross (fol. 80) and the Holy Ghost (fol. 83v)</note>
                     </msItem>
                     <msItem>
                        <note>Penitential Psalms (fol. 88) with litany (fol. 100)</note>
                     </msItem>
                     <msItem>
                        <note>Roman office of the dead (fol. 105v). Additions: notes and
                           instructions on the hearing of mass (fols. 134, 147) in French</note>
                     </msItem>
                    
                     <textLang mainLang="la" otherLangs="frm">Latin, Middle French</textLang>
                  </msItem>
               </msContents>
