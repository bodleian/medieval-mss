               <msContents>
                  <msItem n="1" xml:id="MS_Bodl_169-item1">
                     <author key="person_51685861">Bridget of Sweden</author>
                     <title key="work_1161">Reuelationes (Liber caelestis)</title>
                     <note>(books i-iv)</note>
                     <rubric>Liber celestis Reuelacionum Christi ad sponsam</rubric>
                     <incipit type="prologue">Stupor &amp; mirabilia audita sunt ... Mirabile siquidem erit</incipit>
                     <incipit type="text">Ego sum creator celi</incipit>
                     <explicit>non sedebit cum iudicantibus orbem</explicit>
                     <finalRubric>Explicit liber celestis Reuelacionum Dei quartus. Deo gracias</finalRubric>
                     <note>With lists of chapters and prologue.</note>
                     <!-- the text in SC relating to Richard Lavenham is erroneous -->
                     <textLang mainLang="la">Latin</textLang>
                  </msItem>
               </msContents>
               <physDesc>
                  <objectDesc form="codex">
                     <supportDesc material="chart">
                        <support>paper</support>
                        <extent>viii + 120 leaves <dimensions type="binding" unit="in">
                              <height>12.25</height>
                              <width>8.75</width>
                           </dimensions></extent>
                     </supportDesc>
