                        <incipit>Noxia qui dixit</incipit>
                        <explicit defective="true">defertur ad horas</explicit>
                        <textLang mainLang="la">Latin</textLang>
                     </msItem>
                     <msItem class="#vitae_sanctorum" n="3" xml:id="MS_Bodl_596-part3-item3">
