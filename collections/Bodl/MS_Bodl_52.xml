                  <msItem n="1" xml:id="MS_Bodl_52-item1">
                     <locus from="1">(fol. 1)</locus>
                     <author key="person_2465074">Richard Rolle</author>
                     <title key="work_4295">Paruum Iob</title>
                     <rubric>Tractatus Ricardi heremite de Hampole super Lecciones Exequiarum</rubric>
                     <incipit>Parce mihi Domine ... Exprimitur autem in hijs verbis</incipit>
                     <note>Followed at fol. by 'Versus eiusdem quos composuit in fine eiusdem tractatus' (4 lines beginning 'Talentum traditum')</note>
                  </msItem>
                  <msItem n="2" xml:id="MS_Bodl_52-item1a">
                     <locus from="59">(fol. 59)</locus>
                     <author key="person_100162520">Innocent IV</author>
                     <title key="work_2198">Moralitas de scaccario</title>
                     <rubric>Moralitas de scacario secundum dominum Innocencium tertium</rubric>
                     <incipit>Mundus iste totus quoddam scacarium est</incipit>
                  </msItem>
                  <msItem class="#legalia" n="3" xml:id="MS_Bodl_52-item2">
                     <locus from="61">(fol. 61)</locus>
                     <author key="person_100194528">Pierre Jean Olivi</author>
                     <title key="work_14969">Tractatus de contractibus, de usuris, de restitutionibus</title>
                     <incipit>Circa vendicionem &amp; empcionem contractus queruntur</incipit>
                     <explicit>Hec de vsuris &amp; restitucionibus sufficiant quoad presens. &amp; Deo gracias</explicit>
                  </msItem>
                  <msItem n="4" xml:id="MS_Bodl_52-item3">
                     <locus from="100v">(fol. 100v)</locus>
                     <author key="person_68939042">John Wycliffe</author>
                     <title key="work_2790">De dissensione paparum siue De schismate</title>
                     <rubric>Tractatus paruus de cismate</rubric>
                     <finalRubric>Tractatus magistri Johannis. W. de cismate</finalRubric>
                  </msItem>
                  <msItem n="5" xml:id="MS_Bodl_52-item4">
                     <locus from="102v">(fol. 102v)</locus>
                     <author key="person_171573392">Simon Alcock</author>
                     <title key="work_4542">De modo diuidendi themata</title>
                     <rubric>Quidam tractatus de modo diuidendi themata pro materia sermonis dilatanda, quem composuit venerabilis doctor in theologia magister Symon Alcok</rubric>
                     <incipit>Notandum est quod nullum potest excogitari thema</incipit>
                  </msItem>
