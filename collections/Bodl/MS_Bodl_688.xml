               <msContents>
                  <textLang mainLang="la">Latin</textLang>
                  <msItem n="1" resp="#SC">
                     <locus>(fols. i-viii)</locus>
                     <msItem>
                        <locus>(fols. i-ii, vii-viii)</locus>
                        <author key="person_100184667">Gregory the Great</author>
                        <title key="work_1869">Homiliae in Ezechielem</title>
                        <note>Four leaves of a 15th cent. MS., including the close of a preceding treatise. Signatures k iii-k vi.</note>
                     </msItem>
                     <msItem>
                        <locus>(fol. iii)</locus>
                        <title type="desc">Theological notes</title>
                        <note>Fols. iv-iv mostly blank.</note>
                     </msItem>
                  </msItem>
                  <msItem n="2" resp="#SC">
                     <msItem n="a">
                        <locus>(fol. 1)</locus>
                        <rubric>Tabula omeliarum Gregorii super Euangelia</rubric>
                        <note>Alphabetical.</note>
                     </msItem>
                     <msItem n="b">
                        <locus>(fol. 4v)</locus>
                        <rubric>Tabula omeliarum Gregorii pape super Ezechielem</rubric>
                        <note>Alphabetical, with explanatory note, followed (fol. 11) by some theological notes; cf. fols. 15, 16v, 34v, 40v.</note>
                     </msItem>
                     <msItem n="c">
                        <locus>(fol. 11v)</locus>
                        <note>Eusebian canons applied to a metrical synopsis of the Gospels</note>
                        <incipit type="preface">Canon grece secundum Ysidorum</incipit>
                        <incipit>Matheus. A generat b magos vocat Egiptum petit exit</incipit>
                     </msItem>
                     <msItem n="d">
                        <locus>(fol. 15)</locus>
                        <note>Table showing which homilies of a above suit particular Sundays 'secundum vsum Sarum.'</note>
                     </msItem>
                     <msItem n="e">
                        <locus>(fol. 16v)</locus>
                        <note>Memorial verses for the books of the Bible.</note>
                        <incipit>Pentateu Generat Exitque Leui Nume Deutro</incipit>
                     </msItem>
                     <msItem n="f">
                        <locus>(fol. 17)</locus>
                        <rubric>Exempla sacre Scripture</rubric>
                        <note>Arranged alphabetically under subjects.</note>
                     </msItem>
                     <msItem n="g">
                        <locus>(fol. 28)</locus>
                        <title key="work_16033" type="desc">Epistle and Gospel list</title>
                        <note>Temporale (fol. 28); sanctorale (fol. 32); common of the saints (fol. 33); votive masses (fol. 34); masses for the dead (fol. 34). Incipits and explicits.</note>
                     </msItem>
                     <msItem n="h">
                        <locus>(fol. 37)</locus>
                        <author key="person_2179">Ps.-Origen</author>
                        <title key="work_4075">Homily on Maria stabat</title>
                        <rubric>Omelia Origenis de Maria Magdalena</rubric>
                        <incipit>In illo tempore Maria...Audiuimus fratres Mariam ad monumentum</incipit>
                     </msItem>
                  </msItem>
