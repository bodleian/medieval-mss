                  <msContents>
                     <msItem class="#medica" n="1" xml:id="MS_Bodl_177-part3-item1">
                        <title key="work_13232" type="desc">Medical treatises</title>
                        <msItem>
                           <locus>(fol. 33)</locus>
                           <rubric>Tracatus vrinarum</rubric>
                           <incipit>In tractatu vrinarum primum quid vrina sit videndum</incipit>
                        </msItem>
                        <msItem>
                           <locus>(fol. 34v)</locus>
                           <rubric>Liber pulsuum</rubric>
                        </msItem>
                        <msItem>
                           <locus>(fol. 38)</locus>
                           <rubric>Modus iudicandi urinas</rubric>
                           <note>‘Secundum galterum’.</note>
                        </msItem>
                        <msItem>
                           <locus>(fol. 38v)</locus>
                           <rubric>Liber vrinarum Egidij cum commento</rubric>
                           <note>The text of the poem is not given.</note>
                        </msItem>
                        <textLang mainLang="la">Latin</textLang>
                     </msItem>
                     <msItem n="2" xml:id="MS_Bodl_177-part3-item2">
                        <title key="work_14987" type="desc">Treatise on chiromancy</title>
                        <incipit>Si linea vite sit grossa</incipit>
                        <incipit>Ad sciendum artem chiromanticam</incipit>
                        <note resp="#MLH">Cf. <ref target="https://cctr1.umkc.edu/find/$db=eTK&amp;ms=0060J">eTK 0060J</ref>, <ref target="https://cctr1.umkc.edu/find/$db=eTK&amp;ms=1108J">1108J</ref>, <ref target="https://cctr1.umkc.edu/find/$db=eTK&amp;ms=1453K">1453K</ref> (?)</note>
                        <textLang mainLang="la">Latin</textLang>
                     </msItem>
                     <msItem n="3" xml:id="MS_Bodl_177-part3-item3">
                        <locus>(fol. 42)</locus>
                        <title key="work_10409" type="desc">Astrological treatise</title>
                        <rubric>Fortuna de xii signis</rubric>
                        <incipit>Qui fuerit natus in signo Aquarij</incipit>
                        <textLang mainLang="la">Latin</textLang>
                     </msItem>
                     <msItem class="#mathematica" n="4" xml:id="MS_Bodl_177-part3-item4">
                        <locus>(fol. 45)</locus>
                        <author key="person_84975906">Johannes de Sacro Bosco</author>
                        <title key="work_2590">Algorismus</title>
                        <note>Ends imperfect in ch. 4 (here ch. 5). Each chapter followed by the corresponding passage of <persName key="person_2583890" role="aut">Alexander de Villa Dei</persName>, <title key="work_409">Algorismus</title>.</note>
                        <textLang mainLang="la">Latin</textLang>
                     </msItem>
                     <msItem class="#astrologia_astronomia #prognostica" n="5" xml:id="MS_Bodl_177-part3-item5">
                        <locus>(fol. 46v)</locus>
                        <title key="work_14987" type="desc">Treatise on chiromancy</title>
                        <incipit>Chiromancia est sciencia cognoscendi</incipit>
                        <textLang mainLang="la">Latin</textLang>
                     </msItem>
                  </msContents>
