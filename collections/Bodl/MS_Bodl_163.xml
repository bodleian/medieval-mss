               <msPart xml:id="MS_Bodl_163-part1">
                  <msIdentifier>
                     <altIdentifier type="partial">
                        <idno type="part">MS. Bodl. 163 – Part 1 (fols. 1–227)</idno>
                     </altIdentifier>
                  </msIdentifier>
                  <msContents>
                     <msItem n="1" xml:id="MS_Bodl_163-part1-item1">
                        <locus>(fol. 1)</locus>
                        <msItem>
                           <author key="person_61539765">Bede</author>
                           <title key="work_968">Ecclesiastical history</title>
                           <textLang mainLang="la">Latin</textLang>
                        </msItem>
                        <msItem class="#versus">
                           <note>On the margin of fol. 152v, half-erased, is Caedmon's hymn </note>
                           <textLang mainLang="ang">Old English</textLang>
                        </msItem>
                        <msItem class="#glossa">
                           <note>There are four Old English glosses, published by A. S. Napier (<title>Old English Glosses</title>, 1900, p. 198), and 15th cent. marginal notes in ink and in lead.</note>
                           <textLang mainLang="ang" otherLangs="la">Old English and Latin</textLang>
                        </msItem>
                     </msItem>
                     <msItem class="#historica">
                        <note>At fol. 209 follows a 12th cent. note on Ethelred II and Edmund Ironside.</note>
                        <textLang mainLang="la">Latin</textLang>
                     </msItem>
                     <msItem n="2" xml:id="MS_Bodl_163-part1-item2">
                        <locus>(fol. 209v)</locus>
                        <author key="person_264407238">Aethelwulf</author>
                        <title key="work_281">De abbatibus</title>
                        <textLang mainLang="la">Latin</textLang>
                     </msItem>
                     <msItem class="#topographica">
                        <note>At fol. 226v follows an extract from <persName key="person_95147024" role="aut">Jerome</persName> and from <persName key="person_2479624" role="aut">Orosius</persName> 'de situ Babylonis'</note>
                        <textLang mainLang="la">Latin</textLang>
                     </msItem>
                     <msItem class="#magica">
                        <note>Fol. 227, a charm </note>
                        <incipit> 'Tigaoð. Tigaoð. Tigaoð. calicet</incipit>
                        <textLang mainLang="la">Latin</textLang>
                     </msItem>
                  </msContents>
                  <physDesc>
                     <objectDesc form="codex">
                        <supportDesc material="perg">
                           <support>parchment</support>
                        </supportDesc>
                     </objectDesc>
                     <handDesc>
                        <handNote>Several hands.</handNote>
                     </handDesc>
                     <decoDesc>
                        <decoNote type="decInit">Good initial, fol. I, the whole page added to a MS. of 11th century. Cf. MS. Laud Misc. 636. (<ref target="https://catalog.hathitrust.org/Record/000468709">Pächt and Alexander</ref> iii. 71, pl. VII)</decoNote>
                     </decoDesc>
                  </physDesc>
