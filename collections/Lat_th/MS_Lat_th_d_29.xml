                           <explicit>ne offense parentur</explicit>
                           <textLang mainLang="la">Latin</textLang>
                        </msItem>
                        <msItem xml:id="MS_Lat_th_d_29-part2-item13-item4">
                           <author key="person_12442856">Eadmer</author>
