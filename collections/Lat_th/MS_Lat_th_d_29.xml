                        <textLang mainLang="la">Latin</textLang>
                     </msItem>
                     <msItem xml:id="MS_Lat_th_d_29-part2-item8">
                        <locus from="57">(fol.57)</locus>
                        <title key="work_14538">Sententiae Anselmi</title>
                        <note>(extracts)</note>
