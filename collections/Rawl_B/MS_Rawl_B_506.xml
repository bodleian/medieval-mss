                  <provenance><persName key="person_45071137" role="fmo">Henry, Earl of Clarendon</persName> (<title>Catalogus Manuscriptorum Angliae</title> 2/2 no. 56).</provenance>
                  <!-- https://archive.org/details/CatalogiLibrorumManuscriptorumAngliae1/page/n1013 -->
                  <provenance><persName key="person_47563478" role="fmo">Richard Rawlinson, 1690–1755</persName>.</provenance>
                  <acquisition>Bequeathed by him to the Bodleian.</acquisition>
               </history>
