                     </handDesc>
                     <decoDesc>
                        <decoNote type="none">None.</decoNote>
                     </decoDesc>
                  </physDesc>
                  <history>
                     <origin>
                        <origDate calendar="Gregorian" notBefore="1495" notAfter="1505">15th century, end or 16th century, beginning</origDate>
