                  <physDesc>
                     <p><secFol>-didisse cognouit (fol. 72ra)</secFol></p>
                     <objectDesc form="codex">
                        <supportDesc material="perg">
                           <support>parchment</support>
                           <extent>
                              <dimensions unit="mm" type="leaf">
                                 <height min="243" max="247">c.243–47</height>
