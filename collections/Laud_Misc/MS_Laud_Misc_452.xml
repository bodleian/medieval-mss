                     <msItem n="1" xml:id="MS_Laud_Misc_452-partA-item1">
                        <author key="person_297475745">John Chrysostom</author>
                        <title type="desc" key="work_2675">Homilies</title>
                        <note>Items. a-nn are the collection of 38 sermons (several pseudonymous) discussed by A. Wilmart, 'La
                           collection des 38 homélies latines de saint Jean Chrysostome', <title>Journal of
                           Theological Studies</title> 19 (1918),  305-327; items oo-qq frequently follow the collection in manuscripts (Wilmart, nos. 39-42).</note>
                        <msItem n="a" xml:id="MS_Laud_Misc_452-itema">
                           <locus from="1v">(fol. 1v)</locus>
                           <note>Sermo in superscriptionem Psalmi L</note>
                           <incipit>Pictores imitantur arte naturam</incipit>
                           <finalRubric>Explicit sancti Johannis episcopi Constantinopolitani omelia
                              de superscriptione psalmi quinquagesimi</finalRubric>
                        </msItem>
                        <msItem n="b" xml:id="MS_Laud_Misc_452-itemb">
                           <locus from="13v">(fol. 13v)</locus>
                           <rubric>Omelia in ipsum quinquagesimum psalmum</rubric>
                           <incipit>Reliquias eternse mensae hodie vobis reddere volo</incipit>
                        </msItem>
                        <msItem n="c" xml:id="MS_Laud_Misc_452-itemc">
                           <locus from="25v">(fol. 25v)</locus>
                           <note>De psalmo centesimo xxii</note>
                           <incipit>Prosunt nonnunquam supplicia</incipit>
                        </msItem>
                        <msItem n="d" xml:id="MS_Laud_Misc_452-itemd">
                           <locus from="27">(fol. 27)</locus>
                           <note>De psalmo cl</note>
                           <incipit>Laudate Dominum in Sanctis ejus ; alius inquit in Sancto ejus,
                              alius in Sanctificatione ejus</incipit>
                        </msItem>
                        <msItem n="e" xml:id="MS_Laud_Misc_452-iteme">
                           <locus from="28v">(fol. 28v)</locus>
                           <note>De beato Job</note>
                           <incipit>Verumtamen fratres carissimi </incipit>
                        </msItem>
                        <msItem n="f" xml:id="MS_Laud_Misc_452-itemf">
                           <locus from="29v">(fol. 29v)</locus>
                           <note>De ascensione Heliae</note>
                           <incipit>Apud quosdam veteres reges moris erat</incipit>
                        </msItem>
                        <msItem n="g" xml:id="MS_Laud_Misc_452-itemg">
                           <locus from="32">(fol. 32)</locus>
                           <rubric>In natali Machabeorum</rubric>
                           <incipit>Quam speciosa et grata</incipit>
                        </msItem>
                        <msItem n="h" xml:id="MS_Laud_Misc_452-itemh">
                           <locus from="38">(fol. 38)</locus>
                           <note>De tribus pueris </note>
                           <incipit> Trium puerorum sermo tractabitur</incipit>

                        </msItem>
                        <msItem n="i" xml:id="MS_Laud_Misc_452-itemi">
                           <locus from="39v">(fol. 39v)</locus>
                           <note>De Sancta Susanna </note>
                           <incipit> Diu in electiones et sancta oracula</incipit>
                        </msItem>
                        <msItem n="k" xml:id="MS_Laud_Misc_452-itemk">
                           <locus from="41">(fol. 41)</locus>
                           <note>De proditione Judae </note>
                           <incipit> Paucis hodie necessarium</incipit>

                        </msItem>
                        <msItem n="l" xml:id="MS_Laud_Misc_452-iteml">
                           <locus from="48v">(fol. 48v)</locus>
                           <note>De cruce et latrone sermones duo</note>
                           <incipit>Hodierna die noster Dominus</incipit>
                           <incipit>Hodie incipiamus </incipit>
                        </msItem>
                        <msItem n="m" xml:id="MS_Laud_Misc_452-itemm">
                           <locus from="58v">(fol. 58v)</locus>
                           <note>De cruce Dominica</note>
                           <incipit>Quid dicam ? quid loquar ? </incipit>
                        </msItem>
                        <msItem n="n" xml:id="MS_Laud_Misc_452-itemn">
                           <locus from="63">(fol. 63)</locus>
                           <note>De ascensione Salvatoris</note>
                           <incipit>Quando de cruce nostra</incipit>
                        </msItem>
                        <msItem n="o" xml:id="MS_Laud_Misc_452-itemo">
                           <locus from="69v">(fol. 69v)</locus>
                           <note>De Pentecoste</note>
                           <incipit> Hodie nobis terra</incipit>
                        </msItem>
                        <msItem n="p" xml:id="MS_Laud_Misc_452-itemp">
                           <locus from="74">(fol. 74)</locus>
                           <note>De nativitate Domini</note>
                           <incipit> Et reclinavit eum in praesepio</incipit>
                        </msItem>
                        <msItem n="q" xml:id="MS_Laud_Misc_452-itemq">
                           <locus from="77">(fol. 77)</locus>
                           <rubric>Iterum de nativitate Domini et Johannis Baptiste et conceptionis
                              de solestitia et aequinoctio conceptionis et nativitatis Domini nostri
                              Jhesu Christi et Johannis Baptistae</rubric>
                           <incipit> Nescio an quisquam ausus sit</incipit>
                        </msItem>
                        <msItem n="r" xml:id="MS_Laud_Misc_452-itemr">
                           <locus from="83">(fol. 83)</locus>
                           <note>De Lazaro resuscitato</note>
                           <incipit>Grandi fratres stupore grandi miraculo</incipit>
                        </msItem>
                        <msItem n="s" xml:id="MS_Laud_Misc_452-items">
                           <locus from="84">(fol. 84)</locus>
                           <note>De Chananea sub figura persecutionis</note>
                           <incipit> Multae tempestates et quietudo aeris</incipit>
                        </msItem>
                        <msItem n="t" xml:id="MS_Laud_Misc_452-itemt">
                           <locus from="91v">(fol. 91v)</locus>
                           <note>In istud S. Matthaei 'Vae mundo ab scandalis;' etc. </note>
                           <incipit> Videtur quidem specialiter</incipit>
                        </msItem>
                        <msItem n="u" xml:id="MS_Laud_Misc_452-itemu">
                           <locus from="93">(fol. 93)</locus>
                           <note>De principibus Marci</note>
                           <incipit> Animal illud quod in Johannis apocalypsin</incipit>
                        </msItem>
                        <msItem n="w" xml:id="MS_Laud_Misc_452-itemw">
                           <locus from="97">(f. 97)</locus>
                           <note>In Lucam Evangelistam de Lazaro et divite</note>
                           <incipit> Homo quidam erat dives</incipit>
                        </msItem>
                        <msItem n="x" xml:id="MS_Laud_Misc_452-itemx">
                           <locus from="102">(fol. 102)</locus>
                           <note>In S. Johannem Evangelistam</note>
                           <incipit> De Johanne dicitur, fuit homo missus a Deo</incipit>
                        </msItem>
                        <msItem n="y" xml:id="MS_Laud_Misc_452-itemy">
                           <locus from="105">(fol. 105)</locus>
                           <note>De recipiendo Severiano episcopo</note>
                           <incipit> Sicuti capita corpus coherere</incipit>
                        </msItem>
                        <msItem n="z" xml:id="MS_Laud_Misc_452-itemz">
                           <locus from="106">(fol. 106)</locus>
                           <note>Sermo de pace, cum susceptus esset Severianus ab episcopo
                              Johanne</note>
                           <incipit> In adventu Domini</incipit>
                        </msItem>
                        <msItem n="aa" xml:id="MS_Laud_Misc_452-itemaa">
                           <locus from="107">(fol. 107)</locus>
                           <note>De jejuniis</note>
                           <incipit> Jocundum quidem nautis</incipit>
                        </msItem>
                        <msItem n="bb" xml:id="MS_Laud_Misc_452-itembb">
                           <locus from="111v">(fol. 111v)</locus>
                           <note>De eruditione disciplinae</note>
                           <incipit>Eruditio disciplinae custos est spei</incipit>
                        </msItem>
                        <msItem n="cc" xml:id="MS_Laud_Misc_452-itemcc">
                           <locus from="113v">(fol. 113v)</locus>
                           <note>Ad Eutropium</note>
                           <incipit> Semper quidem sed nunc</incipit>
                        </msItem>
                        <msItem n="dd" xml:id="MS_Laud_Misc_452-itemdd">
                           <locus from="117">(fol. 117)</locus>
                           <note>Sermo cum de expulsione ejus ageretur</note>
                           <incipit> Multi quidem fluctus</incipit>
                        </msItem>
                        <msItem n="ee" xml:id="MS_Laud_Misc_452-itemee">
                           <locus from="119v">(fol. 119v)</locus>
                           <note>Ad Theodorum monachum </note>
                           <incipit> Si fletus possit et gemitus</incipit>

                        </msItem>
                        <msItem n="ff" xml:id="MS_Laud_Misc_452-itemff">
                           <locus from="125v">(fol. 125v)</locus>
                           <note>De militia spiritali. </note>
                           <incipit> Bona quidem sunt et utilia</incipit>

                        </msItem>
                        <msItem n="gg" xml:id="MS_Laud_Misc_452-itemgg">
                           <locus from="128">(fol. 128)</locus>
                           <note>De militia Christiana</note>
                           <incipit> Omnes homines qui sese student</incipit>
                        </msItem>
                        <msItem n="hh" xml:id="MS_Laud_Misc_452-itemhh">
                           <locus from="130v">(fol. 130v)</locus>
                           <note>De patre et duobus filiis</note>
                           <incipit> Omnium quidem de scripturis</incipit>
                        </msItem>
                        <msItem n="ii" xml:id="MS_Laud_Misc_452-itemii">
                           <locus from="134v">(fol. 134v)</locus>
                           <note>Ad Neophytos </note>
                           <incipit> Benedictus Deus ; ecce stelle</incipit>

                        </msItem>
                        <msItem n="kk" xml:id="MS_Laud_Misc_452-itemkk">
                           <locus from="137v">(fol. 137v)</locus>
                           <note>De Turture </note>
                           <incipit> Turtura matrix heremi</incipit>
                        </msItem>
                        <msItem n="ll" xml:id="MS_Laud_Misc_452-itemll">
                           <locus from="140">(fol. 140)</locus>
                           <note>Sermo quando de Asia recessus est</note>
                           <incipit> Moyses magnus ille Dei</incipit>
                        </msItem>
                        <msItem n="mm" xml:id="MS_Laud_Misc_452-itemmm">
                           <locus from="142">(fol. 142)</locus>
                           <note>Sermo post exilii prioris reditum</note>
                           <incipit>Quid dicam? quid loquar? </incipit>
                        </msItem>
                        <msItem n="nn" xml:id="MS_Laud_Misc_452-itemnn">
                           <locus from="143">(fol. 143)</locus>
                           <note>De fide in Christo </note>
                           <incipit> Si credis quod Deus erat Verbum</incipit>
                        </msItem>
                        <msItem n="oo" xml:id="MS_Laud_Misc_452-itemoo">
                           <locus from="144v">(fol. 144v)</locus>
                           <title key="work_2648">Quod non laeditur homo nisi a seipso</title>
                           <incipit> Scio quod crassioribus</incipit>
                        </msItem>
                        <msItem n="pp" xml:id="MS_Laud_Misc_452-itempp">
                           <locus from="155v">(fol. 155v)</locus>
                           <title key="work_2647">De cordis compunctione libri duo </title>
                           <incipit> Cum te intueor, beate Demetri</incipit>
                        </msItem>
                        <msItem n="qq" xml:id="MS_Laud_Misc_452-itemqq">
                           <locus from="171v">(fol. 171v)</locus>
                           <title key="work_2653">De reparatione lapsi</title>
                           <incipit> Quis dabit capiti etc. Oportunius multo nunc a
                              me</incipit>
                        </msItem>
