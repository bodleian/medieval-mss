                        <author key="person_100184667">Gregory the Great</author>
                        <title key="work_1871">Homiliae XL in euangelia</title>
                     </msItem>
                  </msContents>
                  <physDesc>
                     <objectDesc form="codex">
                        <supportDesc material="perg">
                           <support>Parchment</support>
                           <extent>
                              <dimensions type="leaf" unit="mm">
                                 <height max="272" min="266">c. 266–72</height>
                                 <width max="196" min="189">189–96</width>
                              </dimensions>
                           </extent>
                        </supportDesc>
                        <layoutDesc>
                           <layout columns="2" writtenLines="44">Ruling in ink, two columns of 44 lines. Ruled space <dimensions type="ruled" unit="mm">
                                 <height max="211" min="204">c. 204–11</height>
                                 <width max="137" min="134">134–37</width>
                              </dimensions>
                           </layout>
                        </layoutDesc>
                     </objectDesc>
                     <handDesc hands="1">
                        <handNote resp="#DM" script="textualisNorthern">German textualis by a single hand.</handNote>
                        <handNote resp="#DM">Corrections and marginal annotations, mainly by hands of s. xiv<hi rend="superscript">ex</hi> &amp; s. xv, some of which are clearly from the Mainz Charterhouse. </handNote>
                     </handDesc>
                     <decoDesc>
                        <decoNote resp="#DM" type="rubrication">Red rubrics, majuscules stroked in red.</decoNote>
                     </decoDesc>
                  </physDesc>
                  <history>
                     <origin>
                        <origPlace resp="#DM"><country key="place_7000084">Germany</country></origPlace>
                        <origDate calendar="Gregorian" notAfter="1350" notBefore="1325" resp="#DM">s. xiv<hi rend="superscript">2/4</hi></origDate>
                     </origin>
                     <provenance><orgName key="org_132406363" role="fmo">Mainz, Charterhouse St Michael</orgName>: the 15th-century ex-libris inscriptions (upper margin of fol. 1br, and, in the same hand, fol. 76v).</provenance>
                  </history>
               </msPart>
