                     <msItem n="14" xml:id="MS_Laud_Misc_527-part5-item1">
                        <locus from="115">(fol. 115)</locus>
                        <author key="person_2308">R. de Stanington</author>
                        <title key="work_4167">Compilatio quaedam librorum naturalium</title>
                        <incipit>Postquam precessit rememoracio nostra de elementis, dicendum est de impressionibus</incipit>
                        <explicit>et hec de impressionibus sufficiant</explicit>
                        <finalRubric>Expliciunt notabilia super quartum 
                           librum Metheororum</finalRubric>
                        <note>Followed by tables of vices and virtues, metrical feet, etc., and a note of a solar eclipse in 1330 written 
                           "per manus fratris <persName key="person_3921">Simonis de Burton, canonicum de Bridelingtona</persName>".</note>
                        <textLang mainLang="la">Latin</textLang>
                     </msItem>
                     <msItem class="#auctoritates" n="15" xml:id="MS_Laud_Misc_527-part5-item2">
                        <locus from="127r" to="158r">(fol. 127r–158r)</locus>
                        <author key="person_281346627">Defensor</author>
                        <title key="work_1376">Liber scintillarum</title>
                        <rubric>Liber Sententiarum de diversis voluminibus</rubric>
                        <rubric> Cassiodorus de karitate</rubric>
                        <incipit>Dominus 
                           dicit in Evangelio Majorem <sic>karicatatem</sic> nemo habet</incipit>
                        <explicit>Deo hostia ipsi fuerimus</explicit>
                        <finalRubric>Hic explicit liber</finalRubric>
                        <textLang mainLang="la">Latin</textLang>
                     </msItem>
                     <msItem>
                        <note>Fols. 158r-159v: theological  notes, 13th century</note>
                        <textLang mainLang="la">Latin</textLang>
                     </msItem>
