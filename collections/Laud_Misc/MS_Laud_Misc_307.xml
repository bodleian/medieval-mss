                        <origPlace cert="low">
                           <country key="place_1000080">Italian</country> or <country key="place_1000070">French</country> (?)</origPlace>
                     </origin>
                     <provenance resp="#NFP" when="1502">Identifiable as N 15 in the <orgName key="org_133214228" role="fmo">Eberbach</orgName> library catalogue of 1502; Eberbach ex libris: Liber Beate Marie Virginis In Erbach (f. 133r)</provenance>
                  </history>
