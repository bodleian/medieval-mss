                  <bindingDesc>
                     <binding notAfter="1639" notBefore="1637">
                        <p>Laudian binding.</p>
                        <p><term ref="http://www.ligatus.org.uk/lob/concept/4913" type="bookmark">Dial bookmark</term>, fol. 230.</p>
                        <!-- check that this link is correct -->
                     </binding>
                  </bindingDesc>
