                        <extent>ii + i 187 + ii leaves
                           <dimensions unit="mm" type="leaf">
                              <height>280</height>
                              <width>190</width>
                           </dimensions>
                        <dimensions unit="mm" type="ruled">
                           <height quantity="220" precision="low">c. 220</height>
                           <width precision="low" quantity="120">c. 120</width>
                        </dimensions></extent>
                     </supportDesc>
                  </objectDesc>
