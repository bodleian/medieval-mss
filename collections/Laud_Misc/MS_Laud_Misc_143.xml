                  <textLang mainLang="la">Latin</textLang>
                  <msItem n="1" xml:id="MS_Laud_Misc_143-item1">
                     <locus from="1va" to="108vb">(fol. 1va–108vb)</locus>
                     <author key="person_66806872">Augustine</author>
                     <title key="work_822">Tractatus in euangelium Iohannis</title>
                     <finalRubric>Explicit expositio sancti Augustine episcopi in evangelium secundum Johannem</finalRubric>
                     <note>Sermons 37–124. Sermons 1–36 are in <bibl subtype="internal" type="ms">MS. Laud Misc. 144</bibl>.</note>
                  </msItem>
                  <msItem n="2" xml:id="MS_Laud_Misc_143-item2">
                     <locus from="109ra" to="113ra">(fol. 109ra–113ra)</locus>
                     <author key="person_2290">Ps.-Ambrose</author>
                     <title key="work_3831">Libellus de dignitate sacerdotali</title>
                     <rubric>Incipit sermo exhortationis beati Ambrosii Christi de informatione pontificis</rubric>
                     <finalRubric>Explicit sermo exhortationis beati episcopi de institucione pontificis</finalRubric>
                  </msItem>
                  <msItem n="3" xml:id="MS_Laud_Misc_143-item3">
                     <locus from="113rb" to="123rb">(fol. 113rb–123rb)</locus>
                     <author key="person_95147024">Jerome</author>
                     <title key="work_2422">Epitaphium S. Paulae uiduae (ep. 108)</title>
                     <rubric>Incipit vita sancte Paulae edita a beato Ieronimo presbytero</rubric>
                     <!-- 33.2-3 and 34 follow the final rubric -->
                  </msItem>
                  <msItem class="#vitae_sanctorum" n="4" xml:id="MS_Laud_Misc_143-item4">
                     <locus from="123va" to="126rb">(fol. 123va–126rb)</locus>
                     <title key="work_12910" type="desc">Passion of St Vincent</title>
                     <incipit>Probabile satis est ad gloriam vincencii</incipit>
                     <explicit>cumulatus ipse sit consecratus ut sit nomen eius benedictum in secula seculorum amen</explicit>
                     <note>Cf. <bibl type="repertory"><ref target="https://clavis.brepols.net/clacla/OA/link.aspx?clavis=BHL&amp;number=8628"><title>BHL</title> 8628</ref>, 8631</bibl>.</note>
                  </msItem>
                  <msItem class="#vitae_sanctorum" n="5" resp="#MLH" xml:id="MS_Laud_Misc_143-item5">
                     <locus from="126rb" to="127va">(fol. 126rb–127va)</locus>
                     <author key="person_2290">Ps.-Ambrose</author>
                     <title key="work_6214">Passio Agnetis</title>
                     <note>(extract)</note>
                     <rubric>H<ex>ec</ex> pars passionis beate agnetis. inseri<supplied>e</supplied>nda his que in breuiario de ea continentur</rubric>
                     <incipit>Beata agnes dixit Noli infantiam <sic>corpolalem</sic> ita in me despicere</incipit>
                     <explicit>in confinio agelli beatissime uirginis agnetis</explicit>
                     <note>i.6 - ii.13 according to the divisions in Acta Sanctorum (AASS Jan. II, 715–17; PL 17.737–41)</note>
                  </msItem>
                  <msItem class="#vitae_sanctorum" n="6" xml:id="MS_Laud_Misc_143-item6">
                     <locus from="127va" to="139rb">(fol. 127va–139rb)</locus>
                     <title key="work_16265" type="desc">Passion of SS Julianus, Basilissa, and companions</title>
                     <note>(<bibl type="repertory"><ref target="https://clavis.brepols.net/clacla/OA/link.aspx?clavis=BHL&amp;number=4529"><title>BHL</title> 4529</ref></bibl>)</note>
                     <note>Fol. 139v blank.</note>
                  </msItem>
                  <msItem n="7" xml:id="MS_Laud_Misc_143-item7">
                     <locus from="140ra" to="147vb">(fol. 140ra–147vb)</locus>
                     <author key="person_102324857">Fulgentius of Ruspe</author>
                     <title key="work_1586">De ueritate praedestinationis</title>
                     <note>(extracts)</note>
                     <msItem>
                        <rubric>Sanctus fulgentius in libro secundo de ueritate predestinatinis et gratie</rubric>
                        <incipit>Veruntamen et orandi et operandi</incipit>
                        <explicit>adeptus est ut doceret</explicit>
                        <note>II.xvii.29 - xviii.30 (PL 65.643–4)</note>
                     </msItem>
                     <msItem>
                        <locus from="140va">(fol. 140va)</locus>
                        <rubric>Incipit liber de predestinatione</rubric>
                        <incipit>Jam nunc de illis Deus quos predestinavit</incipit>
                        <!-- <incipit>Gratia autem Dei vita eterna in Christo Jhesu Domino nostro; Ergo per gratiam Dei justificantur impii</incipit>
                        check what Q says here
                        -->
                        <explicit>Quotquot autem receperunt eum, dedit eis potestatem filios Dei fieri, his, qui credunt in nomine ejus</explicit>
                        <note>III.i.1 - xvii.27 (PL 65.651–666)</note>
                     </msItem>
                  </msItem>
                  <msItem n="8" xml:id="MS_Laud_Misc_143-item8">
                     <locus from="147vb" to="155rb">(fols. 147vb–155rb)</locus>
                     <author key="person_102324857">Fulgentius of Ruspe</author>
                     <title key="work_1585">Ad Monimum</title>
                     <note>(bk. 1)</note>
                     <rubric>Incipit prologus libri sancti Fulgentii ad Monimum de fide</rubric>
                     <note>With prologue.</note>
                  </msItem>
