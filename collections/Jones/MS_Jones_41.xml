                  <msItem n="1" xml:id="MS_Jones_41-item-1">
                     <locus from="2">(fols. 2r-6r)</locus>
                     <author key="person_84000827">Eusebius Gallicanus</author>
                     <title key="work_5770">Sermo 24 de beato latrone</title>
                     <rubric>Expliciit sermo beati Augustini de beato Latrone</rubric>
                     <note>Imperfect at beginning.</note>
                  </msItem>
                  <msItem n="2" xml:id="MS_Jones_41-item-2">
                     <locus from="6v" to="21v">(fols. 6v-21v)</locus>
                     <author key="person_7386286">Ps.-Augustine</author>
                     <title key="work_6283">De praedestinatione et gratia Dei</title>
                     <rubric>Incipit liber beati Augustini de predestinacione diuina</rubric>
                     <incipit>Cum sacrarum voluminibus litterarum</incipit>
                     <explicit>voragine decidamus. Explicit.</explicit>
                  </msItem>
                  <msItem n="3" xml:id="MS_Jones_41-item-3">
                     <locus from="22r" to="26ar">(fols. 22r-26(a) recto)</locus>
                     <author key="person_102305735">Caesarius of Arles (Ps.-Augustine)</author>
                     <title key="work_5773"> Sermo de decem plagis et decem praeceptis </title>
                     <rubric>Incipit liber Augustini de adaptacione decem plagarum contra decem precepta</rubric>
                     <incipit>Non est sine causa</incipit>
                     <explicit>ad terram repromissionis poteritis feliciter peruenire auxiliante domino nostro I. C. qui ...</explicit>
                  </msItem>
                  <msItem n="4" xml:id="MS_Jones_41-item-4">
                     <locus from="26ar" to="39r">(fols. 26(a) verso - 39r)</locus>
                     <author key="person_66806872">Augustine</author>
                     <title key="work_5772">De patientia</title>
                     <rubric>Incipit epistola beati Augustini de pacientia.</rubric>
                     <incipit>Virtus animi que paciencia dicitur</incipit>
                     <explicit>Explicit: et donatum est caritati. Amen.</explicit>
                     <finalRubric>Explicit Augustine de pacientia</finalRubric>
                     <!--<bibl>CPL 308; CSEL 41 (1900), pp. 661–91.</bibl>-->
                  </msItem>
                  <msItem n="5" xml:id="MS_Jones_41-item-5">
                     <locus from="40">(fols. 39v-43v)</locus>
                     <author key="person_84000827">Eusebius Gallicanus (Ps.-Augustine)</author>
                     <title key="work_6846">Sermo 9 de symbolo</title>
                     <rubric>Incipit Augustinus de Simbolo.</rubric>
                     <incipit>Sicut nonnullis scire permissum est</incipit>
                     <explicit defective="true">gloria personarum</explicit>
                     <note>Ends imperfect; four leaves missing after fol. 43.</note>
                     <bibl>CCSL 101 (1971) 97–108, CPPM 1.4625</bibl>
                  </msItem>
                  <msItem n="6" xml:id="MS_Jones_41-item-6">
                     <locus from="48">(fols. 48r-55v)</locus>
                     <author key="person_7386286">Pseudo-Augustine</author>
                     <title key="work_5771">De assumptione beatae Mariae</title>
                     <rubric>Prefatio beati Augustini in episcopi in sermonem de assumptione beate Marie virginis</rubric>
                     <incipit>Ad interrogata de uirginis et matris domini resolutione temporali et assumptione perhenni quid intelligam responsurus</incipit>
                     <incipit>Quia profundissime</incipit>
                     <explicit>obsecro tu et tui qui cum deo patre ... </explicit>
                     <finalRubric>Explicit Augustini sermo de assumptione beate Marie</finalRubric>
                     <bibl>PL 40. 1141–8.</bibl>
                  </msItem>
                  <msItem n="7" xml:id="MS_Jones_41-item-7">
                     <locus from="56">(fols. 56r-92r)</locus>
                     <author key="person_66806872">Augustine</author>
                     <title key="work_6286">Contra epistolam fundamenti Manicheorum</title>
                     <rubric>Augustinus contra Epistolam fundamenti <add>Manichaeorum</add></rubric>
                     <incipit>Unum verum deum</incipit>
                     <explicit> atque adiuvante arguantur</explicit>
                     <finalRubric>Explicit Augustinus contra epistolam fundamenti Manicheorum</finalRubric>
                  </msItem>
                  <msItem n="8" xml:id="MS_Jones_41-item-8">
                     <locus from="92v">(fols. 92v-124r)</locus>
                     <author key="person_66806872">Augustine</author>
                     <title key="work_787">De utilitate credendi</title>
                     <rubric>Augustinus ad amicum contra Manicheos de vtilitate credendi.</rubric>
                     <incipit>Si michi honorate</incipit>
                     <explicit>in ceteris promptior</explicit>
                     <finalRubric>Explicit Augustinus de vtilitate credendi</finalRubric>
                  </msItem>
                  <msItem n="9" xml:id="MS_Jones_41-item-9">
                     <locus from="124v" to="143v">(fols. 124v-143v)</locus>
                     <author key="person_66806872">Augustine</author>
                     <title key="work_747">De agone Christiano</title>
                     <rubric>Augustinus de <sic>angone</sic> Christiano</rubric>
                     <incipit>Corona victorie</incipit>
                     <explicit>simplicitate et sinceritate lactati</explicit>
                     <note>Imperfect at end.</note>
                  </msItem>
                  
