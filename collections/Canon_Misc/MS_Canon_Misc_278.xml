               <msPart xml:id="MS_Canon_Misc_278_129-176">
                  <msIdentifier>
                     <altIdentifier>
                        <idno>MS. Canon. Misc. 278 – Part 4 (fols 129–176)</idno>
                     </altIdentifier>
                  </msIdentifier>
                  <msContents>
                     <summary>Bilingual educational treatise containing several texts</summary>
                     <textLang mainLang="frm" otherLangs="dum">Middle French and Middle Dutch</textLang>
                     <msItem xml:id="MS_Canon_Misc_278_129-176_item1" n="1">
                        <locus from="129r" to="154v">(fols 129–154r)</locus>
                        <title type="desc" key="work_16553">Ene Leere van hoveschede (bilingual educational treatise)</title>
                        <incipit><supplied>O</supplied> benoite sainte trinite</incipit>
                        <incipit><supplied>O</supplied> ghebenedide helgh drievuldicheit</incipit>
                        <explicit>Tu maintendras quant temps sera</explicit>
                        <explicit>zulstu diij beleeden als tijt sal zijn</explicit>
                        <bibl>Edited in de Vreese 1933.</bibl>
                     </msItem>
                     <msItem xml:id="MS_Canon_Misc_278_129-176_item2" n="2">
                        <locus from="154r" to="155v">(fols 154r–155v)</locus>
                        <title type="desc">Bilingual instructions on counting</title>
                        <incipit><supplied>S</supplied>e tu veulx aprendre A compter par la lettre</incipit>
                        <incipit><supplied>E</supplied>nde wilstu leeren T Reekenen bij der lettren </incipit>
                        <explicit>Cest tout</explicit>
                        <explicit>Hets al.</explicit>
