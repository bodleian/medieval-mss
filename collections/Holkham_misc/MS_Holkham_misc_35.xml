               <msContents>
                  <msItem n="1" xml:id="MS_Holkham_misc_35-item1">
                     <locus>(fol. 1r-)</locus>
                     <rubric>Epitaphium Virgilii quod moriens fecit</rubric>
                     <incipit>Mantua me genuit, Calabri rapuere tenet nunc</incipit>
                     <note>(<ref target="http://webserver.erwin-rauner.de/php_ancarm/ancarm_refwerke.php?opus=1000&amp;var=WIC&amp;nr_char=10656"><title>WIC</title> 10656</ref>).</note>
                     <textLang mainLang="la">Latin</textLang>
                  </msItem>
                  <msItem n="2" xml:id="MS_Holkham_misc_35-item2">
                     <note>Four distichs pr. Baehrens, <title>Poet. Lat. Min.</title> IV.156–7:</note>
                     <msItem n="2a" xml:id="MS_Holkham_misc_35-item2-item1">
                        <incipit>Nocte pluit tota redeunt spectacula mane</incipit>
                     </msItem>
                     <msItem n="2b" xml:id="MS_Holkham_misc_35-item2-item2">
                        <incipit>Hos ego versicuos feci tulit alter honores</incipit>
                        <note>[with two lines not in the edition]</note>
                     </msItem>
                     <msItem n="2c" xml:id="MS_Holkham_misc_35-item2-item3">
                        <incipit>Humor alit segetem segeti contratius humor</incipit>
                     </msItem>
                     <msItem n="2d" xml:id="MS_Holkham_misc_35-item2-item4">
                        <incipit>Monte sub hoc lapidum tegitur Balista sepultus</incipit>
                     </msItem>
                     <textLang mainLang="la">Latin</textLang>
                  </msItem>
                  <msItem n="3" xml:id="MS_Holkham_misc_35-item3">
                     <rubric>Versos <note>(sic)</note> Ovidii Nasonis</rubric>
                     <incipit>Vergilius magno quantum concessit Homero</incipit>
                     <note>(<ref target="http://webserver.erwin-rauner.de/php_ancarm/ancarm_refwerke.php?opus=1000&amp;var=WIC&amp;nr_char=20188"><title>WIC</title> 20188</ref>)</note>
                     <textLang mainLang="la">Latin</textLang>
                  </msItem>
                  <msItem n="4" xml:id="MS_Holkham_misc_35-item4">
                     <rubric>Ordo librorum breviter prenotatus</rubric>
                     <incipit>Primus habet Lybicam veniant ut Troes in urbem</incipit>
                     <note>(<ref target="http://webserver.erwin-rauner.de/php_ancarm/ancarm_refwerke.php?opus=1000&amp;var=WIC&amp;nr_char=14661"><title>WIC</title> 14661</ref>).</note>
                     <textLang mainLang="la">Latin</textLang>
                  </msItem>
                  <msItem n="5" xml:id="MS_Holkham_misc_35-item5">
                     <author key="person_8194433">Virgil</author>
                     <title key="work_4920">Aeneid</title>
                     <note> Wanting VIII.450 to the end. Each book is preceded by one of the pseudo Ovidian prologues (pr. Baehrens, <hi rend="italic">Poet. Lat. Min.</hi>, IV, 162–8)</note>
                     <textLang mainLang="la">Latin</textLang>
                  </msItem>
