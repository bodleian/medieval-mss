               <msContents resp="#Q">
                  <msItem n="1" xml:id="MS_Canon_Class_Lat_260-item1">
                     <locus from="3">(fol. 3)</locus>
                     <author key="person_25395604">Valerius Maximus</author>
                     <title key="work_4894">Memorabilia</title>
                     <note>With table of chapters.</note>
                     <textLang mainLang="la">Latin</textLang>
                  </msItem>
                  <msItem n="2" xml:id="MS_Canon_Class_Lat_260-item3">
                     <locus from="123v">(fol. 123v)</locus>
                     <author key="person_79076696">Festus</author>
                     <title key="work_4523">Breuiarium rerum gestarum populi Romani</title>
                     <textLang mainLang="la">Latin</textLang>
                  </msItem>
                  <msItem n="3" xml:id="MS_Canon_Class_Lat_260-item2">
                     <locus from="127v">(fol. 127v)</locus>
                     <author key="person_98031958">Walter Map</author>
                     <title key="work_4964">Epistola Valerii ad Rufinum ne ducat uxorem</title>
                     <note>(= De nugis curialium, iv 3–5).</note>
                     <finalRubric>Expliciunt dissuasiones Valerii ad Rufum, ne ducat uxorem, vel secundum quosdam epistola beati Jeronimi ad eundem</finalRubric>
                     <textLang mainLang="la">Latin</textLang>
                  </msItem>
                  <msItem n="4" xml:id="MS_Canon_Class_Lat_260-item4">
                     <locus from="130v">(fol. 130v)</locus>
                     <title type="desc">Moral sentences from <persName key="person_90637919" role="aut">Seneca</persName>, <persName key="person_66806872" role="aut">Augustine</persName>, <persName key="person_78769600" role="aut">Cicero</persName>, <persName key="person_66462384" role="aut">Terence</persName> and others.</title>
                     <incipit>Miserum est timere cum speres nihil</incipit>
                     <textLang mainLang="la">Latin</textLang>
                  </msItem>
                  <msItem n="5" xml:id="MS_Canon_Class_Lat_260-item5">
                     <locus from="132">(fol. 132)</locus>
                     <title key="work_12638" type="desc">Consolatory letter</title>
                     <note>Anonymous; concerning the recipient's father, also the writer's maternal grandfather.</note>
                     <incipit>Ansie quidem et eo dolore, qui filium decere</incipit>
                     <textLang mainLang="la">Latin</textLang>
                  </msItem>
                  <msItem resp="#Q #PA">
                     <note>Item 5 is preceded by extracts made by Canonici from a diary of <persName key="person_95652639">Altobelli Averoldi, bishop of Pola</persName>, for 1516–1523, headed 'Copia di quanto ho trovato scritti di pugno del Vescovo Altobello Averoldo sulle prime pagine d' un Antioo Valerio Massimo, manuscritto di raggione del S. Co. Ignazio Zanardis a me da esso dona terni ?' </note>
                     <textLang mainLang="it" otherLangs="la">Italian and Latin</textLang>
                  </msItem>
