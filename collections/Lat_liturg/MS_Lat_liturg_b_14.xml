               <msContents>
                  <textLang mainLang="la">Latin</textLang>
                  <msItem class="#liturgica" n="1" xml:id="MS_Lat_liturg_b_14-item1">
                     <title key="work_10664" type="desc">Breviary, Use of Sarum (noted choir Breviary, 'The Denchworth Breviary')</title>
                     <msItem xml:id="MS_Lat_liturg_b_14-item1-item1">
                        <locus>(fol.1)</locus>
                        <note>Temporale</note>
                        <note>(wanting from Nones on the feast of the Innocents to the vigil of the circumcision, due to loss of leaves after fol.34)</note>
                     </msItem>
                     <msItem xml:id="MS_Lat_liturg_b_14-item1-item2">
                        <locus>(fol.146)</locus>
                        <note>Office of the Virgin</note>
                     </msItem>
                     <msItem xml:id="MS_Lat_liturg_b_14-item1-item3">
                        <locus>(fol.149)</locus>
                        <note>Common of the dedication</note>
                     </msItem>
                     <msItem xml:id="MS_Lat_liturg_b_14-item1-item4">
                        <locus>(fol.154v)</locus>
                        <note>Rubrics for the votive office of the local saint and responsorial ‘historiae’</note>
                        <note>A table of dates for their use between August and December was added in a later hand on fol.iiiv</note>
                     </msItem>
                     <msItem xml:id="MS_Lat_liturg_b_14-item1-item5">
                        <locus>(fol.157)</locus>
                        <note>Calendar </note>
                        <note>(containing St. Anne, which suggests a date after 1383)</note>
                     </msItem>
                     <msItem xml:id="MS_Lat_liturg_b_14-item1-item6">
                        <locus>(fol.163)</locus>
                        <note>Ferial psalter</note>
                     </msItem>
                     <msItem xml:id="MS_Lat_liturg_b_14-item1-item7">
                        <locus>(fol.207v)</locus>
                        <note>Lenten litany</note>
                     </msItem>
                     <msItem xml:id="MS_Lat_liturg_b_14-item1-item8">
                        <locus>(fol.210) (fol.220v)</locus>
                        <note>Sanctorale</note>
                        <note>(wanting the end of SS. Fabian and Sebastian, and the beginning of St. Agnes)</note>
                     </msItem>
                     <msItem xml:id="MS_Lat_liturg_b_14-item1-item9">
                        <locus>(fol.329)</locus>
                        <note>Common of the saints</note>
                        <note>ends imperf. in the matins of a virgin not a martyr</note>
                     </msItem>
                     <msItem xml:id="MS_Lat_liturg_b_14-item1-item10">
                        <locus>(fol.351b)</locus>
                        <note>Tones for the Venite </note>
                        <note>beg. imperf</note>
                     </msItem>
                     <msItem xml:id="MS_Lat_liturg_b_14-item1-item11">
                        <locus>(fol.358)</locus>
                        <note>Tonale of the psalm tones</note>
                        <note>A later hand has filled up fol.163 with the blessings at Matins and fol.328v with the memoria of All Saints.</note>
                     </msItem>
                  </msItem>
