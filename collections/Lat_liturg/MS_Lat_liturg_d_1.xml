               <msContents>
                  
                  <msItem class="#liturgica" n="1" xml:id="MS_Lat_liturg_d_1-item1">
                     <title key="work_10910" type="desc">Choir book</title>
                     <note>From a church dedicated to St Lawrence</note>
                     
                     <msItem>
                        <title type="desc">Tones of the Venite exultemus</title>
                     </msItem>
                     
                     <msItem>
                        <locus>(fol. 26v)</locus>
                        <title type="desc">Lamentations</title>
                     </msItem>
                     
                     <msItem>
                        <title type="desc">Two series of Credo</title>
                     </msItem>
                     
                     <msItem>
                        <locus>(fol. 40v)</locus>
                        <title type="desc">Sanctus and Agnus dei</title>
                        <note>For 2 voices, the last Sanctus troped with 'O quam dulciter voces'</note>
                     </msItem>
                     
                     <msItem>
                        <locus>(fol. 50v)</locus>
                        <title type="desc">Ab eterno pater verbum generans</title>
                        <note>In two parts</note>
                     </msItem>
                     
                     <msItem>
                        <locus>(fol. 52)</locus>
                        <title type="desc">Blessing of the font on Holy Saturday</title>
                     </msItem>
                     
                     <msItem>
                        <locus>(fol. 63v)</locus>
                        <title type="desc">A nocturn of the dead</title>
                        <note>With psalms, antiphons and prayers for the burial service</note>
                     </msItem>
                     
                     <msItem>
                        <locus>(fol. 84)</locus>
                        <title type="desc">Troped Benedicamus</title>
                        <note>Among which one 'patrono nostro sancto Laurentio'</note>
                     </msItem>
                     
                     <msItem>
                        <title type="desc">The last Benedicamus in two parts</title>
                     </msItem>
                        
                     <textLang mainLang="la">Latin</textLang>
                  </msItem>
                  
               </msContents>
