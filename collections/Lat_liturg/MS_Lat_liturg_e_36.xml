                     <textLang mainLang="la">Latin</textLang>
                  </msItem>
                  <msItem xml:id="MS_Lat_liturg_e_36-item19">
                     <locus from="26v" to="27r" type="inferredEnd">(fol.26v)</locus>
                     <title key="work_10187">Anima Christi</title>
                     <note>(version)</note>
                     <incipit>Sanctissima anima Christi sanctifica me</incipit>
                     <note>followed by the Anima Christi itself</note>
                     <textLang mainLang="la">Latin</textLang>
                  </msItem>
