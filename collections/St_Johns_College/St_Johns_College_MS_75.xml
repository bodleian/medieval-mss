                     <origPlace>
                        <country key="place_7002445">England</country>
                     </origPlace>
                  </origin>
                  <provenance notBefore="1275" notAfter="1325">‘<persName role="sgn" key="person_4759">Henr’ Wad’</persName> <persName  role="sgn" key="person_4760">eadward Roch</persName> et <persName  role="sgn" key="person_4761">blake
