                  <msItem class="#theologica #sermones" n="1" xml:id="MS_Add_C_263-item1">
                     <locus>(fol. 2)</locus>
                     <author key="person_16017787">Innocent III</author>
                     <title key="work_2195"> Tractatus super septem psalmos pentitentiales</title>
                     <rubric><supplied>T</supplied>ractatus dompni Innocentii pape super septem Psalmos
                        penitentiales</rubric>
                     <incipit>Ne inter occupationes</incipit>
                     <incipit>Cum omne tempus</incipit>
                  </msItem>
                  <msItem class="#theologica #sermones" n="2" xml:id="MS_Add_C_263-item2">
                     <locus>(fol. 55)</locus>
                     <author key="person_4136834">William de Montibus</author>
                     <title key="work_5041">Numerale</title>
                     <rubric>Incipit Summa secundum progressum numerorum a magistro
                        Willelmo cancellario Lincolniensi composita</rubric>
                     <incipit>Audi Israel ... Deus
                        unus est. Contra apostolus ait</incipit>
                     <explicit>Cuius summe finem facimus cui
                        nomen Numerale imponimus. Plura quidem omissa in aliis opusculis nostris
                        copiosius sunt exarata . Explicit</explicit><note> Fols. 94-7 are
                        stained.</note>
                  </msItem>
                  <msItem class="#theologica #sermones" n="3" xml:id="MS_Add_C_263-item3">
                     <locus>(fol. 96v)</locus>
                     <author key="person_4136834">William de Montibus</author>
                     <title key="work_7195">Similitudinarium</title>
                     <incipit>Ad declarandam in sermone</incipit>
                     <incipit>Amor terrenus inuiscat animam</incipit>
                  </msItem>
                  <msItem class="#theologica #sermones" n="4" xml:id="MS_Add_C_263-item4">
                     <locus>(fol. 115)</locus>
                     <author key="person_4136834">William de Montibus</author>
                     <title key="work_5038">De errorum eliminatione</title>
                     <incipit>Ad eliminandum quorundam errores</incipit>
                     <incipit>Decollatio Iohannis Baptiste</incipit>
                  </msItem>
                  <msItem class="#theologica #sermones" n="5" xml:id="MS_Add_C_263-item5">
                     <locus>(fol. 118)</locus>
                     <title type="desc" key="work_14583">Sermons</title>
                     <note>Two sermons, perhaps by the same.</note>
                     <incipit>Christus passus est pro nobis etc. Et si Christus id est unctus</incipit>
                     <incipit>Nisi granum frumenti etc. Non dicit putrefactum.'</incipit>
