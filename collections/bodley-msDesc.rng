         <group>
      
            <zeroOrMore>
               <ref name="model.global"/>
            </zeroOrMore>
      
            <oneOrMore>
               <group>
        
                  <ref name="model.addrPart"/>
        
        
                  <zeroOrMore>
                     <ref name="model.global"/>
                  </zeroOrMore>
        
               </group>
            </oneOrMore>
         </group>
         <ref name="att.global.attributes"/>
         <empty/>
      </element>
