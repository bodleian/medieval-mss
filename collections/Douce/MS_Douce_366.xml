               <msContents resp="#VD">
                  <msItem class="#liturgica #biblia" n="1" xml:id="MS_Douce_366-item1">
                     <title key="work_14103" type="desc">Psalter ('The Ormesby Psalter')</title>
                     <msItem>
                        <locus from="2">(fol. 2)</locus>
                        <title type="desc">Calendar</title>
                        <note>Added, c. 1325.</note>
                     </msItem>
                     <msItem>
                        <locus from="8">(fol. 8)</locus>
                        <title type="desc">Easter table</title>
                        <note>With dots and a pricking at the years 1290, 1295 and 1300</note>
                     </msItem>
                     <msItem resp="#VD #MLH">
                        <locus from="10">(fol. 10)</locus>
                        <title type="desc">Psalter</title>
                        <note>Textual divisions at Pss. 1 (fols. 9v-10r), 26 (fol. 38r), 38 (fol. 55v), 51 (fol. 71v), 52 (fol. 72r), 68 (fol. 89r), 80 (fol. 109r), 97 (fol. 128r), 101 (fol. 131r) and 109 (fol. 147v).</note>
                     </msItem>
                     <msItem resp="#MLH #VD">
                        <locus from="189v">(fol. 189v)</locus>
                        <title type="desc">Weekly canticles, each followed by a collect; no rubrics</title>
                        <msItem>
                           <locus from="189v">(fol. 189v)</locus>
                           <title>Confitebor tibi domine</title>
                        </msItem>
                        <!-- collect, fol. 189v, Deus qui nobis per prophetarum ora precepisti' -->
                        <msItem>
                           <locus from="190r">(fol. 190r)</locus>
                           <title>Ego dixi</title>
                        </msItem>
                        <!-- followed by a collect (?), fol. 191r, 'Deus qui famulo tuo ezechie regi ter qui nos...' -->
                        <msItem>
                           <locus from="191r">(fol. 191r)</locus>
                           <title>Exultauit cor meum</title>
                        </msItem>
                        <!-- followed by a collect (?), fol. 192r, 'Omnipotens sempiterne deus seps unica mundi...' -->
                        <msItem>
                           <locus from="192r">(fol. 192r)</locus>
                           <title>Cantemus domino</title>
                        </msItem>
                        <!-- collect, fol. 193v, 'deus cuius antiqua miracula etiam nostris seculis -->
                        <msItem>
                           <locus from="194r">(fol. 194r)</locus>
                           <title>Domine audiui</title>
                        </msItem>
                        <!-- collect, fol. 195v, 'Deus qui in omnibus ecclesie teu filiis sanctorum prophetarum uoce manifestas' -->
                        <msItem>
                           <locus from="196r">(fol. 196r)</locus>
                           <title>Audite celi</title>
                        </msItem>
                        <!-- collect, fol. 199v, 'Deus gloriatio fidelium et uita iustorum qui per moysen' -->
                     </msItem>
                     <msItem resp="#VD #MLH">
                        <locus from="199v">(fol. 199v)</locus>
                        <title type="desc">Daily canticles, prayers and creeds, each except the Te Deum followed by a collect; no rubrics</title>
                        <msItem>
                           <locus from="199v">(fol. 199v)</locus>
                           <title>Te deum</title>
                        </msItem>
                        <msItem>
                           <locus from="201r">(fol. 201r)</locus>
                           <title>Benedicite omnia opera</title>
                        </msItem>
                        <!-- collect, fol. 201v, Deus qui tribus pueris mitigasti flammas ignium -->
                        <msItem>
                           <locus from="202r">(fol. 202r)</locus>
                           <title>Benedictus dominus deus</title>
                        </msItem>
                        <!-- collect, fol. 202v, 'Deus qui conspicis quia nos undique mala contristant -->
                        <msItem>
                           <locus from="202v">(fol. 202v)</locus>
                           <title>Magnificat</title>
                        </msItem>
                        <!-- collect, fol. 203r, 'Singularis meriti sola sine exemplo mater' -->
                        <msItem>
                           <locus from="203r">(fol. 203r)</locus>
                           <title>Nunc dimittis</title>
                        </msItem>
                        <!-- collect, f. 203v, 'Omnipotens sempiterne deus fac nost quos tibi et  -->
                        <msItem>
                           <locus from="203v">(fol. 203v)</locus>
                           <title type="desc">Quicumque vult</title>
                        </msItem>
                        <!-- collect, 'Perpetus nos quesumus domine pace custodi  -->
                     </msItem>
                     <msItem resp="#MLH #VD">
                        <locus from="206r">(fol. 206r)</locus>
                        <title type="desc">Litany</title>
                        <note>Followed by collects:</note>
                        <msItem>
                           <incipit>Deus cui proprium est misereri semper et parcere</incipit>
                        </msItem>
                        <msItem>
                           <incipit>Omnipotens sempiterne deus qui facis mirabilia magna solus</incipit>
                        </msItem>
                     </msItem>
                     <msItem resp="#VD #MLH">
                        <locus from="209v">(fol. 209v)</locus>
                        <title type="desc">Litany of Norwich cathedral</title>
                        <note>Added, c. 1325.</note>
                        <note>Followed by collects (fol. 212r):</note>
                        <msItem>
                           <incipit>Deus cui proprium est misereri semper et parcere</incipit>
                        </msItem>
                        <msItem>
                           <incipit>Mentem famuli episcopi nostri q<ex>uesumu</ex>s omnipotens deus lumine tue ueritatis illustra </incipit>
                        </msItem>
                        <msItem>
                           <incipit>Omnipotens sempiterne deus qui facis mirabilia magna solus</incipit>
                        </msItem>
                        <msItem>
                           <incipit>Pretende domine famulis et famulabus tuis dexteram celestis auxilii</incipit>
                        </msItem>
                        <msItem>
                           <incipit>Ure igne sancti spiritus renes nostros et cor nostrum domine</incipit>
                        </msItem>
                        <msItem>
                           <incipit>Acciones nostras q<ex>uesumu</ex>s domine aspirando preueni </incipit>
                        </msItem>
                        <msItem>
                           <incipit>Adesto domine suppicationibus nostris et viam famulorum tuorum in salutis tue</incipit>
                        </msItem>
                        <msItem>
                           <incipit>Ecclesie tue domine preces placatus admitte ut destructis aduersitatibus inimici</incipit>
                        </msItem>
                        <msItem>
                           <incipit>A domo tua q<ex>uesumu</ex>s domine spirituales nequicie repellantur</incipit>
                        </msItem>
                        <msItem>
                           <incipit>Deus a quo sancta desideria recta consilia et iusta sunt</incipit>
                        </msItem>
                        <msItem>
                           <incipit>Deus qui inter apostolicos sacerdotes famulos tuos pontificali fecisti</incipit>
                        </msItem>
                        <msItem>
                           <incipit>Animabus quesumus domine famulorum famularumque tuarum oracio proficiat supplicancium</incipit>
                        </msItem>
                        <msItem>
                           <incipit>Deus qui es sanctorum tuorum splendor mirabilis atque lapsorum sulleuator</incipit>
                        </msItem>
                     </msItem>
                     <textLang mainLang="la">Latin</textLang>
                  </msItem>
