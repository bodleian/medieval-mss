               <msContents resp="#VD">
                  <msItem class="#liturgica" n="1" xml:id="MS_Douce_40-item1">
                     <title key="work_10565" type="desc">Book of Hours, Use of Rome</title>
                     <msItem n="1">
                        <locus>(fol.1 )</locus>
                        <note>Calendar with many saints</note>
                     </msItem>
                     <msItem n="2">
                        <locus>(fol. 13)</locus>
                        <note>Pericopes from the gospels (fol. 13) and passion according to St John (fol. 21)</note>
                     </msItem>
                     <msItem n="3">
                        <locus>(fol. 37)</locus>
                        <note>Office of the Virgin and mass (fol. 114)</note>
                     </msItem>
                     <msItem n="4">
                        <locus>(fol. 123)</locus>
                        <note>Penitential Psalms with litany (fol. 135v)</note>
                     </msItem>
                     <msItem n="5">
                        <locus>(fol. 146)</locus>
                        <note>Quicumque</note>
                     </msItem>
                     <msItem n="6">
                        <locus>(fol. 151)</locus>
                        <note>Offices of the dead, the Holy Cross (fol. 198v), and the Holy Ghost (fol. 230)</note>
                     </msItem>
                     <msItem n="7">
                        <locus>(fol. 235)</locus>
                        <note>Litany of the Virgin</note>
                     </msItem>
                     <msItem n="8">
                        <locus>(fol. 242)</locus>
                        <note>Quicumque (cf. above)</note>
                     </msItem>
                     <msItem n="9">
                        <locus>(fol. 247)</locus>
                        <note>Prayers one in Italian.</note>
                     </msItem>
                     <textLang mainLang="la" otherLangs="it">Latin, with a little Italian</textLang>
                  </msItem>
